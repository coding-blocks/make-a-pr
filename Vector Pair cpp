#include <iostream>
#include<utility>
using namespace std;
int main()
{
    pair<int,char> P;
    string x="abcdefghijkl";
    int k=1;
    for(int i=0;i<x.length();i++)
    {
        P.first=k;
        P.second=x[i];
        k+=1;
        cout<<P.first<<","<<P.second<<endl;
    }
 /*   output console will show:1,a
                               2,b.....
    }*/
}
